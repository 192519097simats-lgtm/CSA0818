numbers = [2, 4, 6, 2, 8, 4, 2, 10, 6, 4]

# Create an empty dictionary to store frequencies
frequency = {}

# Count frequency of each element
for num in numbers:
    if num in frequency:
        frequency[num] += 1
    else:
        frequency[num] = 1

# Display result
print("Frequency of each element:")
for key, value in frequency.items():
    print(key, ":", value)

A = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

B = [
    [9, 8, 7],
    [6, 5, 4],
    [3, 2, 1]
]

C = [
    [2, 4, 6],
    [1, 3, 5],
    [7, 9, 11]
]

# Create a result matrix with zeros
result = [[0, 0, 0],
          [0, 0, 0],
          [0, 0, 0]]

# Add the three matrices
for i in range(3):
    for j in range(3):
        result[i][j] = A[i][j] + B[i][j] + C[i][j]

# Display the result
print("Sum of the three matrices:")
for row in result:
    print(row)
